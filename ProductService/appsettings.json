{
  "AWS.Logging": {
    "Region": "us-west-2",
    "LogGroup": "AwsTechSummit.ProductService",
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "XRay": {
    "DisableXRayTracing": "false",
    "AWSXRayPlugins": "EC2Plugin, ECSPlugin",
    "AwsServiceHandlerManifest": "JSONs\\AWSRequestInfo.json",
    "UseRuntimeErrors": "true"
  }
}
